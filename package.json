{
  "name": "@liquidai/leap-web",
  "version": "0.0.1",
  "scripts": {
    "build": "docusaurus build",
    "check": "npm run typecheck && npm run lint",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start",
    "docusaurus": "docusaurus",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\" --log-level=warn",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\" --log-level=warn",
    "postinstall": "husky",
    "lint": "npm run lint:eslint && npm run lint:md",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:eslint:fix": "eslint . --max-warnings 0 --fix",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:md:fix",
    "lint:md": "markdownlint --config .markdownlintrc.yaml '**/*.{md,mdx}'",
    "lint:md:fix": "markdownlint --fix --config .markdownlintrc.yaml '**/*.{md,mdx}'",
    "prepare": "husky",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc --noEmit --p tsconfig.json",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "prettier --write",
    "*.{md,mdx}": "markdownlint --fix --config .markdownlintrc.yaml -f"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^2.0.1",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/plugin-content-docs": "^3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@tabler/icons-react": "^3.35.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@next/eslint-plugin-next": "^14.1.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "22.13.10",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-sql": "^0.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22"
  }
}
